{
	"name": "meta-box/meta-box-builder",
	"description": "Meta Box Builder extension for Meta Box",
	"type": "wordpress-plugin",
	"repositories": [
		{
			"type": "git",
			"url": "git@github.com:wpmetabox/mbb-parser.git"
		}
	],
	"autoload": {
		"psr-4": {
			"MBB\\": "src/",
			"MBB\\SettingsPage\\": "modules/settings-page/src/"
		}
	},
	"require": {
		"twig/twig": "~1.33.2",
		"meta-box/mbb-parser": "dev-master"
	},
	"extra": {
		"light-ignore-plugin": {
			"composer": [
				"LICENSE"
			],
			"twig/twig": [
				"doc",
				"ext",
				"test",
				".editorconfig",
				".gitignore",
				"*.dist",
				".php_cs.dist",
				".travis.yml",
				"*.json",
				"*.rst",
				"CHANGELOG",
				"LICENSE"
			],
			"riimu/kit-phpencoder": [
				"examples",
				"*.md",
				"*.json",
				"LICENSE"
			],
			"meta-box/mbb-parser": [
				".git",
				".gitignore",
				"*.json"
			]
		}
	}
}
